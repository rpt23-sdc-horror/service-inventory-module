{
  "name": "inventory-component",
  "version": "1.0.0",
  "description": "This component is a service from the Front-End-Capstone project. The inventory component displays a size grid, and add to cart functions on the created webpage.",
  "author": "Nathanel Quiocson, Martin Sung",
  "license": "MIT",
  "engines": {
    "node": ">=6.13.0"
  },
  "scripts": {
    "start": "babel-node -x \".js\" -r dotenv/config src/server/index.js",
    "server-dev": "babel-node -x \".js\" src/server/index.js",
    "test": "mocha test/**/*.test.js --require ./test/dom.js --require esm --require ignore-styles --require @babel/register ./test --exit || true",
    "create-postgres-database": "psql -U TO_DO -d postgres -a -f ./src/gateways/schema/schema.sql",
    "seed-postgres": "psql -h TO_DO -d inventory -U TO_DO -a -f ./seeder/postgres_seeder/seed.sql",
    "seed-mongo": "mongoimport --type csv -d inventory -c Products --headerline --drop ./seeder/seed_data/mockData.csv",
    "generate-data": "babel-node -x \".js\" ./seeder/postgres_seeder/data_generator/index.js",
    "react-dev": "webpack -d --watch",
    "load-test": "k6 run ./test/k6-test/k6-test-script.js",
    "build": "webpack --mode production"
  },
  "dependencies": {
    "axios": "^0.20.0",
    "cors": "^2.8.5",
    "csv-write-stream": "^2.0.0",
    "dotenv": "^8.2.0",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "jquery": "^3.5.1",
    "memcached": "^2.2.2",
    "mongoose": "^5.10.7",
    "newrelic": "^7.0.2",
    "pg": "^8.4.1",
    "pgmock2": "^2.0.5",
    "prettier": "^2.1.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/node": "^7.12.6",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.10.4",
    "@babel/register": "^7.12.1",
    "babel-loader": "^8.1.0",
    "babel-preset-react": "^6.24.1",
    "chai": "^4.2.0",
    "css-loader": "^4.3.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "eslint-config-hackreactor": "git://github.com/reactorcore/eslint-config-hackreactor",
    "ignore-styles": "^5.0.1",
    "jsdom": "^16.4.0",
    "mocha": "^8.1.3",
    "nodemon": "^2.0.6",
    "short-uuid": "^4.1.0",
    "sinon": "^9.2.0",
    "style-loader": "^2.0.0",
    "supertest": "^6.0.1",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12"
  }
}
